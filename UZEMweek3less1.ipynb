   "source": [
    "from sympy import Symbol,Limit\n",
    "t=Symbol('t')\n",
    "St=5*t**2 +2*t +8\n",
    "t1=Symbol('t1')\n",
    "delta_t=Symbol('delta_t')\n",
    "St1=St.subs({t:t1})#function S()  b\n",
    "St1_delta=St.subs({t:t1+delta_t}) #a\n",
    "Limit((St1_delta-St1)/delta_t,delta_t,0).doit()  #b-a/delta \n"
   ]
